Pod::Spec.new do |s|
  s.name = 'ChartsUI'
  s.version = '0.1.0'
  s.license = 'MIT'
  s.summary = 'Charts library for SwiftUI'
  s.homepage = 'https://github.com/Kireyin/ChartsUI'
  s.authors = { 'Kireyin' => 'kireyin@icloud.com' }
  s.source = { :git => 'https://github.com/Kireyin/ChartsUI.git', :tag => s.version }

  s.ios.deployment_target = '13.0'

  s.swift_versions = ['5.2']

  s.source_files = 'Sources/*.swift'

end